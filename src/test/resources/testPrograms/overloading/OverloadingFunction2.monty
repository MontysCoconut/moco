// Testing: Overloading of functions
//
// The correct overloaded function in combination with different blocks must
// be called.
//
// Expected output: :No2:i:s:f:b:No1

Bool function():
    print(":No1")
    return false

if true:
    Bool function():
        print(":No2")
        return false

    Bool function(Int x):
        print(":i")
        return false
    Bool function(String x):
        print(":s")
        return false
    Bool function(Float x):
        print(":f")
        return false
    Bool function(Bool x):
        print(":b")
        return false

    function()

    function(42)
    function("Hallo")
    function(1.3)
    function(true)

function()
